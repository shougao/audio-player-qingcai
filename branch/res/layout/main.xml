<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	android:id="@+id/relativeLayout1"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<RelativeLayout
		android:layout_alignParentTop="true"
		android:layout_width="fill_parent"
		android:background="#000000"
		android:id="@id/ViewIndication" android:layout_height="50dip">
		<ImageView
			android:id="@+id/imageView1"
			android:src="@drawable/img_playback_bg"
			android:scaleType="fitXY"
			android:layout_height="fill_parent"
			android:layout_width="fill_parent"></ImageView>
		<LinearLayout
			android:id="@+id/linearLayout1"
			android:layout_width="fill_parent"
			android:layout_height="40dip">
			<ImageButton
				android:src="@drawable/xml_button_btn_press"
				android:id="@id/IndMain"
				android:layout_weight="1.0"
				android:background="#00000000"
				android:layout_height="wrap_content"
				android:layout_width="wrap_content" android:scaleType="fitXY"></ImageButton>
			<ImageButton
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:src="@drawable/xml_button_btn_press"
				android:id="@id/ImgList"
				android:layout_weight="1.0"
				android:background="#00000000" android:scaleType="fitXY"></ImageButton>
			<ImageButton
				android:layout_height="wrap_content"
				android:layout_width="wrap_content"
				android:src="@drawable/xml_button_btn_press"
				android:id="@id/ImgLyric"
				android:layout_weight="1.0"
				android:background="#00000000" android:scaleType="fitXY"></ImageButton>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linearLayout2"
			android:layout_alignBottom="@+id/linearLayout1"
			android:layout_width="fill_parent"
			android:layout_alignParentTop="true"
			android:layout_height="fill_parent">
			<LinearLayout
				android:id="@id/linearLeft"
				android:layout_weight="1.0"
				android:gravity="fill_vertical|center"
				android:layout_height="fill_parent"
				android:layout_width="fill_parent">
				<ImageView
					android:src="@drawable/img_buttom_bt_play"
					android:scaleType="center"
					android:layout_height="fill_parent"
					android:layout_width="fill_parent" android:id="@id/ImgMain"></ImageView>
			</LinearLayout>
			<LinearLayout
				android:id="@id/linearMid"
				android:layout_weight="1.0"
				android:gravity="fill_vertical|center"
				android:layout_width="fill_parent"
				android:layout_height="fill_parent">
				<ImageView
					android:src="@drawable/img_buttom_bt_list"
					android:scaleType="center"
					android:layout_height="fill_parent"
					android:layout_width="fill_parent" android:id="@id/ImgList"></ImageView>
			</LinearLayout>
			<LinearLayout
				android:id="@id/linearRight"
				android:layout_weight="1.0"
				android:gravity="fill_vertical|center"
				android:layout_height="fill_parent"
				android:layout_width="fill_parent">
				<ImageView
					android:src="@drawable/img_buttom_bt_lrc"
					android:scaleType="center"
					android:layout_height="fill_parent"
					android:layout_width="fill_parent" android:id="@id/ImgLyric"></ImageView>
			</LinearLayout>
		</LinearLayout>
	</RelativeLayout>
	<LinearLayout
		android:layout_alignParentBottom="true"
		android:layout_width="fill_parent"
		android:background="@drawable/img_playback_bg"
		android:layout_alignParentLeft="true"
		android:id="@id/PlayControlPanpelTop"
		android:layout_height="50dip">
		<RelativeLayout
			android:id="@id/layoutPlayMode"
			android:layout_height="fill_parent"
			android:layout_width="fill_parent"
			android:layout_weight="1.0">
			<ImageView
				android:id="@id/imgPlayMode"
				android:layout_width="30dip"
				android:layout_centerInParent="true"
				android:scaleType="fitXY"
				android:layout_height="20dip" android:src="@drawable/icon_playmode_normal"></ImageView>
			<ImageButton
				android:src="@drawable/xml_playback_btn_press"
				android:id="@id/IndPlayMode"
				android:layout_height="fill_parent"
				android:layout_width="fill_parent"
				android:scaleType="fitXY"
				android:background="#00000000"></ImageButton>
		</RelativeLayout>
		<RelativeLayout
			android:layout_height="match_parent"
			android:id="@id/layoutPrev"
			android:layout_width="fill_parent"
			android:layout_weight="1.0">
			<ImageButton
				android:src="@drawable/xml_playback_btn_press"
				android:layout_alignParentLeft="true"
				android:layout_height="fill_parent"
				android:layout_width="fill_parent"
				android:background="#00000000"
				android:scaleType="fitXY" android:id="@id/btnPrev"></ImageButton>
			<ImageView
				android:src="@drawable/img_playback_bt_last"
				android:id="@id/imgPrev"
				android:scaleType="fitXY"
				android:layout_centerInParent="true"
				android:layout_height="20dip"
				android:layout_width="30dip"></ImageView>
		</RelativeLayout>
		<RelativeLayout
			android:layout_height="match_parent"
			android:id="@id/layoutPlay"
			android:layout_width="fill_parent"
			android:layout_weight="1.0">
			<ImageButton
				android:src="@drawable/xml_playback_btn_press"
				android:layout_alignParentLeft="true"
				android:id="@id/btnPlay"
				android:scaleType="fitXY"
				android:layout_centerInParent="true"
				android:layout_height="fill_parent"
				android:layout_width="fill_parent"
				android:background="#00000000"></ImageButton>
			<ImageView
				android:src="@drawable/img_playback_bt_play"
				android:id="@id/imgPlay"
				android:layout_centerInParent="true"
				android:layout_height="20dip"
				android:layout_width="30dip"
				android:scaleType="fitXY"></ImageView>
		</RelativeLayout>
		<RelativeLayout
			android:layout_height="match_parent"
			android:id="@id/imgNext"
			android:layout_width="fill_parent"
			android:layout_weight="1.0">
			<ImageButton
				android:src="@drawable/xml_playback_btn_press"
				android:layout_alignParentLeft="true"
				android:id="@id/btnNext"
				android:layout_height="fill_parent"
				android:layout_width="fill_parent"
				android:background="#00000000"
				android:scaleType="fitXY"></ImageButton>
			<ImageView
				android:src="@drawable/img_playback_bt_next"
				android:id="@id/imgNext"
				android:layout_centerInParent="true"
				android:scaleType="fitXY"
				android:layout_height="20dip"
				android:layout_width="30dip"></ImageView>
		</RelativeLayout>
		<RelativeLayout
			android:layout_height="match_parent"
			android:id="@id/layoutMenu"
			android:layout_width="fill_parent"
			android:layout_weight="1.0">
			<ImageButton
				android:src="@drawable/xml_playback_btn_press"
				android:layout_alignParentLeft="true"
				android:layout_height="fill_parent"
				android:layout_width="fill_parent"
				android:background="#00000000"
				android:scaleType="fitXY" android:id="@id/IndMenu"></ImageButton>
			<ImageView
				android:id="@id/ImgMenu"
				android:src="@drawable/img_buttom_bt_menu"
				android:layout_centerInParent="true"
				android:scaleType="fitXY"
				android:layout_height="20dip"
				android:layout_width="30dip"></ImageView>
		</RelativeLayout>
	</LinearLayout>
	<LinearLayout android:orientation="vertical" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_below="@id/ViewIndication" android:layout_alignParentLeft="true" android:layout_above="@id/PlayControlPanpelTop" android:layout_alignParentRight="true" android:id="@id/MusicListLay">
		<ListView android:layout_width="match_parent" android:layout_height="match_parent" android:id="@id/MusicListView"></ListView>
	</LinearLayout>
</RelativeLayout>
